    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.1.6",
    "tsx": "^4.21.0",
